generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  nickname String  @unique
  name     String
  surname  String
  email    String  @unique
  phone    Int     @unique
  admin    Boolean
}